page.remove "#tooltip"
@time_slots.each do |time_slot|
  page.insert_html :bottom, "location#{time_slot.location.id}_#{time_slot.start.strftime("%Y-%m-%d")}_events", :partial => "time_slots/time_slot", :object => time_slot
  page["#timeslot#{time_slot.id}"].visual_effect :highlight
  page << "$('#timeslot#{time_slot.id}').click(function (e) {popup_edit($(this)); return false;});"

  # display alert
  ajax_alert(page, "timeslot created");
end